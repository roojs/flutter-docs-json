            "isDeprecated": false,
            "types": []
        },
        {
            "name": "notifyStatusListeners",
            "type": "",
            "desc": "<p>Calls all the status listeners.<\/p>\n<p>If listeners are added or removed during this function, the modifications\nwill not change which listeners are called during this iteration.<\/p>\n    ",
            "static": false,
            "memberOf": "animation.AnimationLocalStatusListenersMixin",
            "sig": "",
            "params": [
                {
                    "name": "status",
                    "type": "animation.AnimationStatus",
                    "desc": "",
                    "isOptional": true,
                    "href": "",
                    "example": "",
                    "isDeprecated": false
                }
            ],
            "isConstructor": false,
            "href": "animation\/AnimationLocalStatusListenersMixin\/notifyStatusListeners.html",
            "example": "<h2><span>Implementation<\/span><\/h2>\n      <pre class=\"language-dart\"><code class=\"language-dart\">void notifyStatusListeners(AnimationStatus status) {\n  final List&lt;AnimationStatusListener&gt; localListeners = List&lt;AnimationStatusListener&gt;.from(_statusListeners);\n  for (AnimationStatusListener listener in localListeners) {\n    try {\n      if (_statusListeners.contains(listener))\n        listener(status);\n    } catch (exception, stack) {\n      FlutterError.reportError(FlutterErrorDetails(\n        exception: exception,\n        stack: stack,\n        library: 'animation library',\n        context: 'while notifying status listeners for $runtimeType',\n        informationCollector: (StringBuffer information) {\n          information.writeln('The $runtimeType notifying status listeners was:');\n          information.write('  $this');\n        },\n      ));\n    }\n  }\n}<\/code><\/pre>\n    ",
            "isDeprecated": false,
            "types": []
        },
        {
            "name": "removeStatusListener",
            "type": "",
            "desc": "<p>Stops calling the listener every time the status of the animation changes.<\/p>\n<p>Listeners can be added with <a href=\"animation\/AnimationLocalStatusListenersMixin\/addStatusListener.html\">addStatusListener<\/a>.<\/p>\n    ",
            "static": false,
            "memberOf": "animation.AnimationLocalStatusListenersMixin",
            "sig": "",
            "params": [
                {
                    "name": "listener",
                    "type": "animation.AnimationStatusListener",
                    "desc": "",
                    "isOptional": true,
                    "href": "",
                    "example": "",
                    "isDeprecated": false
                }
            ],
            "isConstructor": false,
            "href": "animation\/AnimationLocalStatusListenersMixin\/removeStatusListener.html",
            "example": "<h2><span>Implementation<\/span><\/h2>\n      <pre class=\"language-dart\"><code class=\"language-dart\">void removeStatusListener(AnimationStatusListener listener) {\n  final bool removed = _statusListeners.remove(listener);\n  if (removed) {\n    didUnregisterListener();\n  }\n}<\/code><\/pre>\n    ",
            "isDeprecated": false,
