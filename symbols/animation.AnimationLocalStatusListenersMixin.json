        {
            "name": "addStatusListener",
            "type": "",
            "desc": "<p>Calls listener every time the status of the animation changes.<\/p>\n<p>Listeners can be removed with <a href=\"animation\/AnimationLocalStatusListenersMixin\/removeStatusListener.html\">removeStatusListener<\/a>.<\/p>\n    ",
            "static": false,
            "memberOf": "animation.AnimationLocalStatusListenersMixin",
            "sig": "",
            "params": [
                {
                    "name": "listener",
                    "type": "animation.AnimationStatusListener",
                    "desc": "",
