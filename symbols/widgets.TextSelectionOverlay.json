        {
            "name": "showHandles",
            "type": "",
            "desc": "<p>Shows the handles by inserting them into the <a href=\"widgets\/TextSelectionOverlay\/context.html\">context<\/a>'s overlay.<\/p>\n    ",
            "static": false,
