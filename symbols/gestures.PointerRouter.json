        {
            "name": "route",
            "type": "",
            "desc": "<p>Calls the routes registered for this pointer event.<\/p>\n<p>Routes are called in the order in which they were added to the\nPointerRouter object.<\/p>\n    ",
            "static": false,
            "memberOf": "gestures.PointerRouter",
